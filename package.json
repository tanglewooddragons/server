{
  "name": "tanglewood-server",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=debug node src/index.js | ./node_modules/.bin/bunyan -o short",
    "start:production": "NODE_ENV=production node src/index.js | ./node_modules/.bin/bunyan -o short",
    "db:start": "docker start tanglewood",
    "db:create": "node src/util/createDb",
    "db:clean": "docker stop tanglewood && docker rm tanglewood",
    "test": "NODE_ENV=test mocha test/index",
    "test:watch": "NODE_ENV=test mocha test/index --watch",
    "lint": "eslint src/"
  },
  "repository": "https://github.com/tanglewooddragons/server.git",
  "author": "yoshuro <yoshuro@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.12",
    "dotenv": "^4.0.0",
    "joi": "^13.0.2",
    "jsonwebtoken": "^8.1.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-helmet": "^3.3.0",
    "koa-jwt": "^3.2.2",
    "koa-router": "^7.3.0",
    "node-schedule": "^1.2.5",
    "thinky": "^2.3.9"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0"
  }
}
